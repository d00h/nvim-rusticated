# rusticated fish shell theme by haystackandroid (https://github.com/haystackandroid/rusticated)

# normal text
set fish_color_host              444136
set fish_color_normal            444136
set fish_pager_color_completion  444136
set fish_pager_color_description 444136

# muted text
set fish_color_autosuggestion    5f5a4c
set fish_color_comment           5f5a4c

# reverse muted
set fish_pager_color_progress    d3d2ce --background=#5f5a4c

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#989387
set fish_color_search_match      --background=#989387
set fish_color_selection         --background=#989387

# red errors/warnings
set fish_color_cancel            834742
set fish_color_cwd_root          834742
set fish_color_error             834742

# gold special elements
set fish_color_redirection       645826
set fish_color_escape            645826
set fish_color_operator          645826
set fish_color_end               645826

# green commands/status
set fish_color_command           2c633f
set fish_color_cwd               2c633f
set fish_color_user              2c633f

# cyan parameters
set fish_color_param             006470

# blue quoted strings
set fish_color_quote             3c5a89

# magenta pager name match
set fish_pager_color_prefix      784872

